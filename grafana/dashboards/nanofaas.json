  "panels": [
    {
      "title": "Dispatch / Success Rate (req/s)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (function) (max by (function, instance) (rate(function_dispatch_total{function=~\"$function\"}[1m])))",
          "legendFormat": "{{function}} dispatch",
          "refId": "A"
        },
        {
          "expr": "sum by (function) (max by (function, instance) (rate(function_success_total{function=~\"$function\"}[1m])))",
          "legendFormat": "{{function}} success",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Error / Timeout / Reject / Retry Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "(sum by (function) (max by (function, instance) (rate(function_error_total{function=~\"$function\"}[1m])))) or on(function) (0 * sum by (function) (max by (function, instance) (rate(function_dispatch_total{function=~\"$function\"}[1m]))))",
          "legendFormat": "{{function}} error",
          "refId": "A"
        },
        {
          "expr": "(sum by (function) (max by (function, instance) (rate(function_timeout_total{function=~\"$function\"}[1m])))) or on(function) (0 * sum by (function) (max by (function, instance) (rate(function_dispatch_total{function=~\"$function\"}[1m]))))",
          "legendFormat": "{{function}} timeout",
          "refId": "B"
        },
        {
          "expr": "(sum by (function) (max by (function, instance) (rate(function_queue_rejected_total{function=~\"$function\"}[1m])))) or on(function) (0 * sum by (function) (max by (function, instance) (rate(function_dispatch_total{function=~\"$function\"}[1m]))))",
          "legendFormat": "{{function}} rejected",
          "refId": "C"
        },
        {
          "expr": "(sum by (function) (max by (function, instance) (rate(function_retry_total{function=~\"$function\"}[1m])))) or on(function) (0 * sum by (function) (max by (function, instance) (rate(function_dispatch_total{function=~\"$function\"}[1m]))))",
          "legendFormat": "{{function}} retry",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Latency Avg (ms, dispatch->response)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "1000 * (sum by (function) (max by (function, instance) (rate(function_latency_ms_seconds_sum{function=~\"$function\"}[1m]))) / clamp_min(sum by (function) (max by (function, instance) (rate(function_latency_ms_seconds_count{function=~\"$function\"}[1m]))), 0.001))",
          "legendFormat": "{{function}} avg",
          "refId": "A"
        },
        {
          "expr": "1000 * (sum by (function) (max by (function, instance) (rate(function_e2e_latency_ms_seconds_sum{function=~\"$function\"}[1m]))) / clamp_min(sum by (function) (max by (function, instance) (rate(function_e2e_latency_ms_seconds_count{function=~\"$function\"}[1m]))), 0.001))",
          "legendFormat": "{{function}} e2e-avg",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 8
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Latency p95 / p99 (ms)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "(1000 * sum by (function) (max by (function, instance) (function_latency_ms_seconds{function=~\"$function\",quantile=\"0.95\"}))) or on(function) (0 * sum by (function) (max by (function, instance) (rate(function_dispatch_total{function=~\"$function\"}[1m]))))",
          "legendFormat": "{{function}} p95",
          "refId": "A"
        },
        {
          "expr": "(1000 * sum by (function) (max by (function, instance) (function_latency_ms_seconds{function=~\"$function\",quantile=\"0.99\"}))) or on(function) (0 * sum by (function) (max by (function, instance) (rate(function_dispatch_total{function=~\"$function\"}[1m]))))",
          "legendFormat": "{{function}} p99",
          "refId": "B"
        },
        {
          "expr": "(1000 * sum by (function) (max by (function, instance) (function_queue_wait_ms_seconds{function=~\"$function\",quantile=\"0.95\"}))) or on(function) (0 * sum by (function) (max by (function, instance) (rate(function_dispatch_total{function=~\"$function\"}[1m]))))",
          "legendFormat": "{{function}} queue-wait p95",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 8
          }
        },
        "overrides": []
      }
    },
